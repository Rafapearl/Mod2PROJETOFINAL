<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">

   
    <title>Artigo</title>
</head>
<body>
    <%- include('header'); %>

    <% formula.forEach((formula, find)=> { %>
        <section class="containerCard" class="animation_in">
          <article class="card_content">
            <a class="linken" href="/artigo/<%= find %>">

              <h2 class="nomeHome">
                <%= formula.nome %>
              </h2>
              <h4 class="emailHome">
                <%= formula.email %>
              </h4>
              <h4 class="tituloHome">
                <%= formula.titulo %>
              </h4>
              <p class="publiHome">
                <%= formula.publi %>
              </p>

            </a>

          </article>
        </section>
        <% }) %>
</body>
</html>